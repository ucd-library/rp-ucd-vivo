{
  "spring": {
    "data": {
      "solr": {
        "host": "http://solr-discovery:8983/solr"
      }
    }
  },
  "ui": {
    "url": "http://localhost:4200"
  },
  "vivo": {
    "base-url": "https://localhost:3000/vivo"
  },
  "graphql": {
    "spqr": {
      "gui": {
        "enabled": true
      }
    }
  },
  "middleware": {
