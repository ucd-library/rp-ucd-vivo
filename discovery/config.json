    },
    "export": {
      "individualBaseUri": "http://localhost:3000/display"
    }
  },
  "mail" : {
    "host" : "smtp.gmail.com",
    "username" : "",
    "password" : "",
    "properties" : {
      "mail" : {
        "transport" : {
          "protocol" : "smtp"
        },
        "smtp" : {
          "port" : 587,
          "auth" : true,
          "starttls" : {
            "enable" : true,
            "required" : true
          }
        }
      }
    }
  }
